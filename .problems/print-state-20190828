                          [   g
                            [ n=%$
                                a
                              ~[
                                [ n=%$
                                    v
                                  \/"Innovation is more than endlessly cranking out new and improved gadgets. It also consists of trying to find the best use of the tools you c\/
                                    urrently have lying around the shop. "
                                  \/                                                                                                                                            \/
                                ]
                              ]
                            ]
                            c=~
                          ]
