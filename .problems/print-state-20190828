                          [g=[n=%a a=~[[n=%href v="http://www.opengroup.org/onlinepubs/007908799/xsh/ioctl.html"]]] c=~[[g=[n=%$ a=~[[n=%$ v="ioctl()"]]] c=~]]]
                          [g=[n=%$ a=~[[n=%$ v=" interface and that's a good thing. "]]] c=~]
