                          [   g
                            [ n=%$
                                a
                              ~[
                                [ n=%$
                                    v
                                  \/"If you didn't know to look for it, you may never notice it, but if you've ever gone to get a massage, the therapist (if they're worth their\/
                                     salt) will almost never break their connection with you. "
                                  \/                                                                                                                                            \/
                                ]
                              ]
                            ]
                            c=~
