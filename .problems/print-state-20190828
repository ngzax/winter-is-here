                      [ g=[n=%p a=~]
                          c
                        ~[[g=[n=%$ a=~[[n=%$ v="I love thinking as a function of two axis. A quadrant view of a topic can be surprisingly helpful in understanding it. "]]] c=~]]
                      ]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="I wonder how it would map out if we put most peoples' minds on this map: "]]] c=~]]]
