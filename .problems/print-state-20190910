          [ p=%cracks-in-the-artifice
              q
            [ bone=82
                dat
              [ %.y
                  p
                [ [ creator=~minder-folden
                    title='Cracks in the Artifice'
                    collection=%the-new-era
                    filename=%cracks-in-the-artifice
                    comments=%open
                    date-created=~2019.8.12..14.50.18..a51d
                    last-modified=~2019.8.12..15.19.44..d990
                    pinned=%.n
                  ]
                  [ g=[n=%div a=~]
