          [ p=%maintaining-connection
              q
            [ bone=82
                dat
              [ %.y
                  p
                [ [ creator=~minder-folden
                    title='Maintaining Connection'
                    collection=%random-thoughts
                    filename=%maintaining-connection
                    comments=%open
                    date-created=~2019.8.16..19.31.05..8a37
                    last-modified=~2019.8.16..19.32.39..fd28
                    pinned=%.n
                  ]
                  [ g=[n=%div a=~]
                      c
                    ~[
                      [g=[n=%img a=~[[n=%src v="https://i.imgur.com/pp6Fyhu.jpg"]]] c=~]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="My ex wife was (and still is, I guess) a massage therapist. "]]] c=~]]]
                      [ g=[n=%p a=~]
                          c
                        ~[[g=[n=%$ a=~[[n=%$ v="Being her practice dummy was an especially nice perk of her training, and one day she mentioned a principle that I've long-since recognized as incredibly important across domains: "]]] c=~]]
                      ]
                      [g=[n=%h2 a=~] c=~[[g=[n=%$ a=~[[n=%$ v="Maintain connection."]]] c=~]]]
                      [ g=[n=%p a=~]
                          c
                        ~[
                          [   g
                            [ n=%$
                                a
                              ~[
                                [ n=%$
                                  v="If you didn't know to look for it, you may never notice it, but if you've ever gone to get a massage, the therapist (if they're worth their salt) will almost never break their connection with you. "
                                ]
                              ]
                            ]
                            c=~
                          ]
                        ]
                      ]
                      [ g=[n=%p a=~]
                          c
                        ~[
                          [g=[n=%$ a=~[[n=%$ v="It's very subtle, but if they ignore this point it leads to a very jarring experience when you're floating on the table and then they touch you. That binary switch flip from "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“No connection”"]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v=" to "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“someone's touching me”"]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v=" is really deep in our wiring and it connects directly to the "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“is this situation a threat?”"]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v=" circuit. "]]] c=~]
                        ]
                      ]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="Once the therapist makes connection with you, your mind knows right where they are, what they're doing, and where they're going. There's no surprise. "]]] c=~]]]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="It's not that you're in control, but you settle down because you have a lot of information you wouldn't have otherwise. "]]] c=~]]]
                      [g=[n=%h2 a=~] c=~[[g=[n=%$ a=~[[n=%$ v="Other places"]]] c=~]]]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="Like I said, this principle plays out in many dynamics. "]]] c=~]]]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="One of my favorites is in kung fu. Wing Chun kung fu, to be specific. "]]] c=~]]]
                      [ g=[n=%p a=~]
                          c
                        ~[
                          [g=[n=%$ a=~[[n=%$ v="There's a practice called "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“Chi Sau”"]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v=" which loosely translates to "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“sticky hands”"]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v=" but I prefer the term "]]] c=~]
                          [g=[n=%$ a=~[[n=%$ v="“sensing hands.”"]]] c=~]
