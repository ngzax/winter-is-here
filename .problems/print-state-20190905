                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="They also apply everyWhen, too. Principles have no beginning, nor an end. "]]] c=~]]]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="The idea of start)finish is relegated solely to the physical world. Only "]]] c=~] [g=[n=%$ a=~[[n=%$ v="“things”"]]] c=~] [g=[n=%$ a=~[[n=%$ v=" can be or not be. "]]] c=~]]]
