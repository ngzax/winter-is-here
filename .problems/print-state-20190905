                          ]
                        ]
                      ]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="That's what I love about martial arts. It forces you to be fully present and completely embody yourself. If your mind wanders for a split second, WHAM! "]]] c=~]]]
                      [g=[n=%p a=~] c=~[[g=[n=%$ a=~[[n=%$ v="Down you go. "]]] c=~]]]
